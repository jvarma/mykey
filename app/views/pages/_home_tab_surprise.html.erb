<div class="tab-pane" id="2">

	<div class="container">
		<% @users.each do |user| %>
			<% unless !user.confirmed_at %>
			<div class="row">
				<div class="span2">
					<%= image_tag("default_profile_photo.png")%>
				</div>
				<div class="span10">
					<div class="row">
						<% unless user.profile.nil? %>
							<h3 class="pull-left">
								<%= link_to user.name, user_path(user) %>
							</h3>
						<% else %>
							<h3 class="pull-left">
								<%= link_to user.name %>
							</h3>
						<% end %>
					</div>
					<div class="row">
						<%= pluralize(user.assets.count, "upload")%>
						<% if current_user == user %>
						| <%= link_to "add new", new_asset_path %>
						<% end %>
					</div>
				</div>
			</div>
			<hr />
			<% end %>

		<% end %>

	</div>

	
</div>